{
  "schemaVersion": 1,
  "id": "terracompat",
  "version": "${version}",

  "name": "Terra Compatibility Module",
  "description": "Re-enables vanilla cave and structure generation to ensure compatibility with corresponding mods",
  "authors": [
    "xieve"
  ],
  "contact": {
    "sources": "https://github.com/xieve/terra-compat"
  },

  "license": "LGPL-3.0-or-later",
  "icon": "assets/modid/icon.png",

  "environment": "server",
  "entrypoints": {
    "main": [
      "net.xieve.terracompat.InitStub"
    ]
  },
  "mixins": [
    "terracompat.mixin.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
